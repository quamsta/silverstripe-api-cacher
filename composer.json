{
    "name": "quamsta/silverstripe-api-cacher",
    "description": "Provides a few helper functions for caching API requests made via SilverStripe project/module code.",
    "type": "silverstripe-vendormodule",
    "homepage": "http://github.com/quamsta/silverstripe-api-cacher",
    "keywords": [
        "silverstripe"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {"name": "Dustin Quam","email": "quamsta@gmail.com"}
    ],
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "quamsta\\ApiCacher\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
